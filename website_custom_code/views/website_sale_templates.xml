<odoo>
    <data>
        <template id="kh_price_list_custom_menu" inherit_id="website_sale.template_header_centered_logo"
                  name="kh price list custom menu">
            <xpath expr="//t[@t-call='website_sale.header_cart_link']" position="before">
                <t t-call="website_sale.pricelist_list">
                </t>
            </xpath>
        </template>
        <template id="kh_vat_sign_up" inherit_id="website_sale.address" name="kh vat sign up ">
            <xpath expr="//form[hasclass('checkout_autoformat')]/div/t/t/div[contains(@t-attf-class,'div_vat')]"
                   position="attributes">
                <attribute name="style">display: none !important;</attribute>
            </xpath>
        </template>
        <template id="kh_street_sign_up" inherit_id="website_sale.address" name="kh street sign up ">
            <xpath expr="//form[hasclass('checkout_autoformat')]/div/div[contains(@t-attf-class,'div_street2')]"
                   position="attributes">
                <attribute name="style">display: none !important;</attribute>
            </xpath>
        </template>

        <template id="kh_zip_sec_sign_up" inherit_id="website_sale.address" name="kh zip sec sign up ">
            <xpath expr="//form[hasclass('checkout_autoformat')]/div/t[4]/div[contains(@t-attf-class,'div_zip')]/input[@name='zip']"
                   position="attributes">
                <attribute name="t-att-value">'011'</attribute>
            </xpath>

        </template>
        <template id="kh_zip_sign_up" inherit_id="website_sale.address" name="kh zip sign up ">
            <xpath expr="//form[hasclass('checkout_autoformat')]/div/t[3]/div[contains(@t-attf-class,'div_zip')]/input[@name='zip']"
                   position="attributes">
                <attribute name="t-att-value">'011'</attribute>
            </xpath>
        </template>

        <template id="kh_product_variant" inherit_id="sale.variants" name="kh product variant">
            <xpath expr="//ul/t/li/t[4]/ul/t/li[hasclass('list-inline-item')]" position="replace">
                <t t-set="is_out" t-value="ptav.product_tmpl_id.product_variant_id.is_out_of_stock"/>
                <t t-if="is_out == False">
                    <label class="col-form-label">
                        <div class="form-check">
                            <input type="radio"
                                   t-attf-class="form-check-input js_variant_change #{ptal.attribute_id.create_variant}"
                                   t-att-checked="ptav in combination"
                                   t-att-name="'ptal-%s' % ptal.id"
                                   t-att-value="ptav.id"
                                   t-att-data-value_id="ptav.id"
                                   t-att-data-value_name="ptav.name"
                                   t-att-data-attribute_name="ptav.attribute_id.name"
                                   t-att-data-is_custom="ptav.is_custom"
                                   t-att-data-is_single="single"
                                   t-att-data-is_single_and_custom="single_and_custom"/>
                            <div class="radio_input_value form-check-label">
                                <span t-field="ptav.name"/>
                                <t t-call="sale.badge_extra_price"/>
                            </div>
                        </div>
                    </label>
                </t>
            </xpath>
