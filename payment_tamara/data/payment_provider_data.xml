<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="payment_provider_tamara" model="payment.provider">
        <field name="name">Tamara</field>
        <field name="code">tamara</field>
        <field name="display_as">Pay With Tamara</field>
        <field name="image_128"
               type="base64"
               file="payment_tamara/static/description/icon.png"/>
        <field name="redirect_form_view_id" ref="redirect_form"/>
    </record>
    <record id="payment_icon_tamara" model="payment.icon">
        <field name="sequence">10</field>
        <field name="name">Tamara</field>
        <field name="image" type="base64" file="payment_tamara/static/description/icon.png"/>
    </record>
</odoo>
